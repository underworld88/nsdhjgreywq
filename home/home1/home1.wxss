/* home/home1/home1.wxss */
.background{
  width:100%;
  height:100%;
  position:absolute;
  background-size:100%  100%;
  z-index:-1;
}
.btn {
  width: 80%;
  margin-top: 20rpx;
  background-color: rgb(212, 176, 108);
  color: white;
  border-radius: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 55rpx;
  height: 55rpx;
}

.btn::after {
  border-radius: 98rpx;
}

.tickle{
  width: 80%;
  text-align: center;
  color: rgb(212, 176, 108);
  font-size: 80rpx;
  padding-left: 30px;
  bottom:-50px;
}

